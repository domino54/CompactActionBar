# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = false

# Set default charset
charset = utf-8

# Lua
[*.lua]
insert_final_newline = true
indent_style = space
indent_size = 2

# XML
[*.xml]
indent_style = space
indent_size = 4
